  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+nachoweb3",
      "url": "/blog/protocol-handler?url=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/blog/file-handler",
      "accept": {
        "text/markdown": [".md", ".markdown"],
        "application/json": [".json"]
      }
    }
  ],
  "share_target": {
    "action": "/blog/share-handler",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "screenshots",
          "accept": ["image/png", "image/jpeg", "image/webp"]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate"
}